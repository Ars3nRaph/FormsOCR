{
  "name": "formsocr-backend",
  "version": "1.0.0",
  "description": "FormsOCR Studio - Backend OCR Processing Service",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "docker:build": "docker build -t formsocr-backend .",
    "docker:run": "docker run -p 3001:3001 formsocr-backend"
  },
  "keywords": [
    "ocr",
    "pdf",
    "document-processing",
    "tesseract",
    "rapidocr"
  ],
  "author": "FormsOCR Studio",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0",
    "axios": "^1.11.0",
    "bullmq": "^4.18.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "form-data": "^4.0.4",
    "helmet": "^7.2.0",
    "ioredis": "^5.7.0",
    "joi": "^17.13.3",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pdf2pic": "^3.2.0",
    "rate-limiter-flexible": "^4.0.1",
    "sharp": "^0.32.6",
    "tesseract.js": "^5.1.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
